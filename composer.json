{
    "name": "phpbench/benchmark-containers",
    "license": "mit",
    "description": "This benchmarking suite compares PHP Dependency Injection Containers. Its sort of an example project for PHPBench.",
    "require": {
        "php-di/php-di": "^7.0.0",
        "symfony/dependency-injection": "^4.2|^5",
        "pimple/pimple": "^v3.5.0",
        "illuminate/container": "^8.0.0|^9.0.0",
        "doctrine/cache": "^2.2.0@stable",
        "symfony/config": "^4.2|^5",
        "symfony/filesystem": "^4.2|^5",
        "phpbench/phpbench": "^1.1",
        "league/container": "^4.2",
        "aura/di": "^4.2",
        "laminas/laminas-servicemanager": "^3.20"
    },
    "autoload": {
        "psr-4": {
            "PhpBench\\Benchmarks\\Container\\": "benchmarks",
            "PhpBench\\Benchmarks\\Container\\Acme\\": "lib"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "kylekatarnls/update-helper": true
         }
    }
}
